{
  "name": "drupal/recommended-project",
  "description": "Project template for Drupal 8 projects with a relocated document root",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "homepage": "https://www.drupal.org/project/drupal",
  "support": {
    "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
    "chat": "https://www.drupal.org/node/314178"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "git",
      "url": "https://github.com/National-Forestry-Authority/nfa_tracker"
    },
    {
      "type": "package",
      "package": {
          "name": "national-forestry-authority/nfa-map",
          "version": "2.1.0",
          "type": "drupal-library",
          "dist": {
              "url": "https://github.com/National-Forestry-Authority/nfa-map/releases/download/v2.1.0/v2.1.0-dist.zip",
              "type": "zip"
          },
          "extra": {
              "installer-name": "nfa-map"
          }
      }
    }
  ],
  "require": {
    "ext-zip": "^1",
    "composer/installers": "^1.9",
    "cweagans/composer-patches": "^1",
    "drupal/admin_toolbar": "^3",
    "drupal/auto_entitylabel": "^3",
    "drupal/blocktabs": "^1",
    "drupal/bootstrap": "^3",
    "drupal/business_rules": "^2",
    "drupal/computed_field": "3.x",
    "drupal/config_split": "^1",
    "drupal/core-composer-scaffold": "^9",
    "drupal/core-project-message": "^9",
    "drupal/core-recommended": "^9",
    "drupal/core-vendor-hardening": "^9",
    "drupal/csv_serialization": "^2",
    "drupal/ctools": "^3",
    "drupal/default_content": "^2",
    "drupal/dependent_fields": "^1",
    "drupal/diff": "^1",
    "drupal/entity_reference_revisions": "^1",
    "drupal/entity_reference_views_select": "^1",
    "drupal/field_group": "^3",
    "drupal/flag": "^4",
    "drupal/geocoder": "^3",
    "drupal/geofield": "^1",
    "drupal/inline_entity_form": "^1",
    "drupal/migrate_plus": "^5",
    "drupal/migrate_source_csv": "^3",
    "drupal/migrate_source_ui": "^1",
    "drupal/migrate_spreadsheet": "^2",
    "drupal/migrate_tools": "^5",
    "drupal/paragraphs": "^1",
    "drupal/pathauto": "^1",
    "drupal/serial": "^1",
    "drupal/token": "^1",
    "drupal/views_ajax_history": "^1",
    "drupal/views_autocomplete_filters": "^1",
    "drupal/views_bulk_operations": "^3",
    "drupal/views_data_export": "^1",
    "drupal/views_field_view": "^1",
    "drupal/views_flipped_table": "^1",
    "drupal/views_geojson": "^1",
    "drupal/yearonly": "^9",
    "drush/drush": "^11",
    "national-forestry-authority/nfa-map": "^2.1",
    "national-forestry-authority/nfa_tracker": "dev-main"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/core-composer-scaffold": true,
      "drupal/core-project-message": true,
      "drupal/core-vendor-hardening": true
    }
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      }
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "web/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "web/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "drupal-core-project-message": {
      "include-keys": [
        "homepage",
        "support"
      ],
      "post-create-project-cmd-message": [
        "<bg=blue;fg=white>                                                         </>",
        "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
        "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
        "<bg=blue;fg=white>                                                         </>",
        "",
        "<bg=yellow;fg=black>Next steps</>:",

        "  * Install the site: https://www.drupal.org/docs/8/install",
        "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
        "  * Get support: https://www.drupal.org/support",
        "  * Get involved with the Drupal community:",
        "      https://www.drupal.org/getting-involved",
        "  * Remove the plugin that prints this message:",
        "      composer remove drupal/core-project-message"
      ]
    },
    "patches": {
      "drupal/core": {
        "2648950-253: Use form element of type date instead textfield when selecting a date in an exposed filter": "https://www.drupal.org/files/issues/2021-06-07/2648950-253.patch"
      },
      "drupal/default_content": {
        "2698425-175: Do not reimport existing entities": "https://www.drupal.org/files/issues/2022-07-29/default_content-fix-uuid-duplicate-entry-2698425.patch"
      },
      "drupal/yearonly": {
        "3172745-8: Provide default year options": "https://www.drupal.org/files/issues/2022-12-12/yearonly-default-options-3172745-8.patch"
      },
      "drupal/migrate_tools": {
        "3272434-6: Undefined index: #description_display in template_preprocess_fieldset()": "https://www.drupal.org/files/issues/2022-07-06/3272434-6-fixed-undefined-index-descriptiondisplay.patch"
      }
    }
  },
  "require-dev": {
    "drupal/devel": "^4.2"
  }
}
